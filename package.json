  "scripts": {
    "reinstall": "rm -r ./node_modules && rm package-lock.json && rm package-lock.json && npm i",
    "debug": "set DEBUG=* next dev --host 0.0.0.0",
    "build-server": "cross-env NODE_ENV=development && netlify functions:build --src netlify/functions/functions.js",
    "serve-server": "cross-env NODE_ENV=development && netlify functions:serve",
    "serve-server2": "cross-env NODE_ENV=production && netlify deploy --open",
    "tbuild": "netlify env:import .env.production && next build && netlify functions:build --src netlify/functions",
    "dev": "cross-env NODE_ENV=development next start --host 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "test": "echo \"Write tests! -> https://nextjs.org/docs/testing\" && exit 1",
    "deploy": "npm run build && netlify deploy --prod",
    "ndevelop": "netlify env:import .env.development && netlify dev",
    "develop": "next dev",
    "build": "next build",
    "serve": "next start",
    "lint": "next lint"
  },
